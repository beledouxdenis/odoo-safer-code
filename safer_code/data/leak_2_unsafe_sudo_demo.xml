<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="secret_message" model="mail.message">
            <field name="subject">Your credentials</field>
            <field name="body">
                Hello Mitchell !
                We just created your account. Here are your credentials to connect:
                - Login: admin
                - Password: bananadmin
            </field>
            <field name="model">res.users</field>
            <field name="res_id" eval="ref('base.user_admin')"/>
        </record>

        <record id="collaborative_project" model="safer_code.project">
            <field name="name">Collaborative project</field>
            <field name="collaborator_ids" eval="[Command.link(ref('base.demo_user0'))]"/>
        </record>

        <record id="collaborative_task" model="safer_code.task">
            <field name="name">Collaborative task</field>
            <field name="project_id" eval="ref('collaborative_project')"/>
        </record>

        <record id="secret_attachment" model="ir.attachment">
            <field name="name">mitchell-payslip.pdf</field>
            <field name="datas"></field>
            <field name="res_model">res.users</field>
            <field name="res_id" eval="ref('base.user_admin')"></field>
        </record>

    </data>
</odoo>
